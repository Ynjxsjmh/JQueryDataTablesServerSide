<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
	http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
<display-name>JQueryDataTablesServerSide</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>CompanyJsonMatrixServlet</servlet-name>
    <servlet-class>jquery.datatables.controller.CompanyJsonMatrixServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CompanyGsonMatrixServlet</servlet-name>
    <servlet-class>jquery.datatables.controller.CompanyGsonMatrixServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CompanyGsonObjectsServlet</servlet-name>
    <servlet-class>jquery.datatables.controller.CompanyGsonObjectsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>YadcfServlet</servlet-name>
    <servlet-class>jquery.datatables.controller.YadcfServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>CompanyJsonMatrixServlet</servlet-name>
    <url-pattern>/CompanyJsonMatrix</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CompanyGsonMatrixServlet</servlet-name>
    <url-pattern>/CompanyGsonMatrix</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CompanyGsonObjectsServlet</servlet-name>
    <url-pattern>/CompanyGsonObjects</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>YadcfServlet</servlet-name>
    <url-pattern>/Yadcf</url-pattern>
  </servlet-mapping>
</web-app>